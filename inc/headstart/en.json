            "post_content": "<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"10vh\",\"bottom\":\"10vh\"}}},\"backgroundColor\":\"tertiary\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-tertiary-background-color has-background\" style=\"padding-top:10vh;padding-bottom:10vh;\"><!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:image {\"id\":9,\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https:\/\/winkeldemo.files.wordpress.com\/2022\/02\/pexels-cottonbro-7026777.jpg?w=819\" alt=\"\" class=\"wp-image-9\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:gallery {\"linkTo\":\"none\",\"className\":\"has-lightbox\"} -->\n<figure class=\"wp-block-gallery has-nested-images columns-default is-cropped has-lightbox\"><!-- wp:image {\"id\":10,\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https:\/\/winkeldemo.files.wordpress.com\/2022\/02\/pexels-cottonbro-7026768.jpg?w=809\" alt=\"\" class=\"wp-image-10\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":11,\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https:\/\/winkeldemo.files.wordpress.com\/2022\/02\/pexels-cottonbro-7026767.jpg?w=640\" alt=\"\" class=\"wp-image-11\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:image {\"id\":12,\"sizeSlug\":\"large\",\"linkDestination\":\"none\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https:\/\/winkeldemo.files.wordpress.com\/2022\/02\/pexels-cottonbro-7026775.jpg?w=805\" alt=\"\" class=\"wp-image-12\" \/><\/figure>\n<!-- \/wp:image --><\/figure>\n<!-- \/wp:gallery --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"40px\"}}} -->\n<h2 id=\"the-puffer-jacket\" style=\"font-size:40px;\">The Puffer Jacket<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:heading {\"style\":{\"color\":{\"text\":\"#22222280\"}},\"fontSize\":\"medium\"} -->\n<h2 class=\"has-text-color has-medium-font-size\" id=\"350-00\" style=\"color:#22222280;\">$350.00<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\">Our Puffer Jacket is this winter\u2019s must-have essential item. <br>It has a relaxed fit, fleece-lined pockets, and a drawstring hood to keep you extra warm.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:jetpack\/recurring-payments -->\n<div class=\"wp-block-jetpack-recurring-payments\"><!-- wp:jetpack\/button {\"element\":\"a\",\"text\":\"Buy Now\",\"borderRadius\":0,\"width\":\"100%\",\"className\":\"\"} \/--><\/div>\n<!-- \/wp:jetpack\/recurring-payments -->\n\n<!-- wp:spacer {\"height\":\"40px\"} -->\n<div style=\"height:40px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\"><strong>Description<\/strong><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\">Our Puffer Jacket is this winter\u2019s must-have essential item. <br>It has a relaxed fit, fleece-lined pockets, and a drawstring hood to keep you extra warm.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\">Plus, it's made out of 100% recycled polyester, and machine washable. <\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\"><strong>Shipping<\/strong><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#222222bf\"}},\"fontSize\":\"small\"} -->\n<p class=\"has-text-color has-small-font-size\" style=\"color:#222222bf;\">We ship worldwide with a flat shipping fee.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"style\":{\"spacing\":{\"padding\":{\"top\":\"60px\",\"bottom\":\"60px\"}}},\"layout\":{\"contentSize\":\"600px\",\"wideSize\":\"968px\"}} -->\n<div class=\"wp-block-group\" style=\"padding-top:60px;padding-bottom:60px;\"><!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"lineHeight\":\"1.2\"}}} -->\n<h2 class=\"has-text-align-center\" id=\"this-is-hands-down-the-best-coat-you-can-buy\" style=\"line-height:1.2;\">\u201cThis is hands down the best coat you can buy\u201d<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"typography\":{\"fontSize\":\"14px\"},\"color\":{\"text\":\"#222222bf\"}}} -->\n<p class=\"has-text-align-center has-text-color\" style=\"color:#222222bf;font-size:14px;\">- Jessica L.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group -->",
